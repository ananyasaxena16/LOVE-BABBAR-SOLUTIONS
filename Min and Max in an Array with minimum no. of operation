public static void main(){

  int n = scn.nextInt();

  int[] arr= new int[n];
  for(int i = 0; i<n; i++){
    arr[i] = scn.nextInt();
    }
  int min = arr[0];
  int max = arr[0];
  for(int i = 1;i<arr.length; i++){
    if(arr[i]>max){
      max=arr[i];
    }
    if(arr[i]<min){
      min=arr[i];
    }
  }
  System.out.println(min);
  System.out.println(max);
}
