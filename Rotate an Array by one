class Compute {
    
    public void rotate(int arr[], int n)
    {
        rev(arr,0, n-2);
        rev(arr,0, n-1);
        
    }
    public void rev(int arr[], int i, int j){
        while(i<j)
        {
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
            i++;
            j--;
        }
    }
}
